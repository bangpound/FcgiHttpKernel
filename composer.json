{
    "name": "igorw/fcgi-http-kernel",
    "description": "Adapter from HttpKernelInterface to FastCGI.",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "adoy/fastcgi-client": "dev-master",
        "symfony/http-kernel": "^5.4"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "^5.4",
        "symfony/process": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Igorw\\FcgiHttpKernel\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
